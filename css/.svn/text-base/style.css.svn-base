.button {
	font: 12px Arial, Helvetica, sans-serif;
	padding: 0 3px;
	color: #000;
	background: url(/images/button/bg_menu.gif) repeat-x;
	/*filter: alpha(opacity=70);   transparency for IE; note no decimal point */
	/*for Mozilla*/
 	outline: 1px solid #1590BE ;
	border: 1px solid #FFFFFF !important;
	height: 20px !important;
	line-height: 17px !important;

	/*for IE7*/
	> border: 1px solid #1590BE !important;
	> height: 20px !important;
	> line-height: 17px !important;
}
#toolbar,.toolbar{	
	opacity: .75;               /* standard CSS3 style for transparency */
	-moz-opacity: .75;          /* transparency for older Mozillas */
	filter: alpha(opacity=75);  /* transparency for IE; note no decimal point */
	z-index: 100;
	position: absolute;
}
